<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <pattern id="medicalPattern" patternUnits="userSpaceOnUse" width="100" height="100">
      <!-- Medical cross -->
      <g opacity="0.1" fill="#ffffff">
        <rect x="45" y="20" width="10" height="60"/>
        <rect x="20" y="45" width="60" height="10"/>
      </g>
      <!-- Stethoscope -->
      <g opacity="0.08" fill="#ffffff" transform="translate(20,20)">
        <circle cx="10" cy="10" r="8" fill="none" stroke="#ffffff" stroke-width="2"/>
        <path d="M18 10 Q30 10 30 25 Q30 40 15 40" fill="none" stroke="#ffffff" stroke-width="2"/>
        <circle cx="15" cy="40" r="3"/>
      </g>
      <!-- Heart rate line -->
      <g opacity="0.06" fill="none" stroke="#ffffff" stroke-width="2" transform="translate(60,30)">
        <path d="M0 20 L10 20 L15 5 L20 35 L25 10 L30 25 L35 20 L45 20"/>
      </g>
    </pattern>
  </defs>
  
  <!-- Background gradient -->
  <rect width="100%" height="100%" fill="url(#medicalPattern)"/>
  
  <!-- Additional medical icons scattered -->
  <g opacity="0.05" fill="#ffffff">
    <!-- Clipboard -->
    <g transform="translate(100,100)">
      <rect x="0" y="0" width="30" height="40" rx="3" fill="none" stroke="#ffffff" stroke-width="2"/>
      <rect x="10" y="-5" width="10" height="10" rx="2"/>
      <line x1="5" y1="10" x2="25" y2="10" stroke="#ffffff" stroke-width="1"/>
      <line x1="5" y1="15" x2="25" y2="15" stroke="#ffffff" stroke-width="1"/>
      <line x1="5" y1="20" x2="20" y2="20" stroke="#ffffff" stroke-width="1"/>
    </g>
    
    <!-- Pill -->
    <g transform="translate(200,150)">
      <ellipse cx="15" cy="10" rx="15" ry="8" fill="none" stroke="#ffffff" stroke-width="2"/>
      <line x1="0" y1="10" x2="30" y2="10" stroke="#ffffff" stroke-width="2"/>
    </g>
    
    <!-- Syringe -->
    <g transform="translate(300,200)">
      <rect x="0" y="8" width="25" height="4" fill="none" stroke="#ffffff" stroke-width="1"/>
      <rect x="25" y="6" width="8" height="8" fill="none" stroke="#ffffff" stroke-width="1"/>
      <line x1="33" y1="10" x2="38" y2="10" stroke="#ffffff" stroke-width="2"/>
    </g>
    
    <!-- Thermometer -->
    <g transform="translate(400,120)">
      <line x1="10" y1="0" x2="10" y2="30" stroke="#ffffff" stroke-width="3"/>
      <circle cx="10" cy="35" r="5" fill="#ffffff"/>
    </g>
    
    <!-- Medical bag -->
    <g transform="translate(500,180)">
      <rect x="0" y="10" width="30" height="20" rx="3" fill="none" stroke="#ffffff" stroke-width="2"/>
      <rect x="12" y="5" width="6" height="10" fill="none" stroke="#ffffff" stroke-width="1"/>
      <rect x="10" y="15" width="10" height="8" fill="none" stroke="#ffffff" stroke-width="1"/>
      <line x1="13" y1="17" x2="17" y2="17" stroke="#ffffff" stroke-width="1"/>
      <line x1="15" y1="15" x2="15" y2="19" stroke="#ffffff" stroke-width="1"/>
    </g>
    
    <!-- Bandage -->
    <g transform="translate(150,300)">
      <rect x="0" y="5" width="25" height="10" rx="5" fill="none" stroke="#ffffff" stroke-width="2"/>
      <line x1="8" y1="7" x2="8" y2="13" stroke="#ffffff" stroke-width="1"/>
      <line x1="17" y1="7" x2="17" y2="13" stroke="#ffffff" stroke-width="1"/>
    </g>
    
    <!-- DNA Helix -->
    <g transform="translate(350,350)">
      <path d="M0 0 Q10 10 0 20 Q-10 30 0 40" fill="none" stroke="#ffffff" stroke-width="2"/>
      <path d="M0 0 Q-10 10 0 20 Q10 30 0 40" fill="none" stroke="#ffffff" stroke-width="2"/>
      <line x1="-5" y1="10" x2="5" y2="10" stroke="#ffffff" stroke-width="1"/>
      <line x1="-5" y1="30" x2="5" y2="30" stroke="#ffffff" stroke-width="1"/>
    </g>
  </g>
</svg>